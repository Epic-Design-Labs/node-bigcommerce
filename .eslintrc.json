{
	"root": true,
	"env": {
		"node": true,
		"es2021": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [],
	"rules": {
		"no-func-assign": "off",
		"import/no-extraneous-dependencies": "off"
	},
	"settings": {}
}
